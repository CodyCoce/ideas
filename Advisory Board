/* Tells both tables to be 100% available width and to have the background color be transparent */
#top-table, #bottom-table {width: 100%; background-color:transparent;}

/* Tells the <tr>s of both tables to have the background color of white */
#top-table tr, #bottom-table tr {background-color:white;}

/* Tells the #-table-tds of both tables to be centered, have a 0.3 second transition and have no border */
#top-table #top-table-td, #bottom-table #bottom-table-td {
	text-align:center;
	transition:0.3s;
	border:none;
}

/* Basically makes that shadowy effect when hovering over each box */
#top-table #top-table-td div:hover, #bottom-table #bottom-table-td div:hover {box-shadow:0px 0px 30px 2px #888;}

/* Whitespace area */
#top-table #top-table-td-whitespace {padding:1px;}

/* Targets images in both the top and bottom tables and tells them to be the same size and have the same border */
#top-table img, #bottom-table img {
    border:solid 5px #606060;
	width:200px;
	height:200px;
}

/* Tells the all the <span> tags in both tables to have this specific color */
#top-table span, #bottom-table span {color:#c35967;}

/* Some whitespace for the bottom table */
#bottom-table #bottom-table-td-two {padding:1px;width:4px;}
</style>

